<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:custom="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.alandy.mytopbar.MainActivity">

    <com.alandy.mytopbar.TopBar
        android:id="@+id/topbar"
        android:layout_width="match_parent"
        android:layout_height="40dp"

        custom:leftBackground="@drawable/adward_selector"
        custom:leftText="左侧"
        custom:leftTextColor="#ffffff"
        custom:rightBackground="@drawable/set_selector"
        custom:rightText="右侧"
        custom:rightTextColor="#ffffff"
        custom:myTitle="自定义标题"
        custom:titleTextColor="#00ff00"
        custom:titleTextSize="10sp">


    </com.alandy.mytopbar.TopBar>
</RelativeLayout>
